    "history": "^5.3.0",
    "html-webpack-plugin": "^5.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lodash.difference": "^4.5.0",
    "lodash.omit": "^4.5.0",
    "mini-css-extract-plugin": "^2.6.1",
    "node-fetch": "^3.2.10",
