    "lint:fix": "npm run lint -- --fix",
    "preview": "vite preview",
    "start": "vite",
    "storybook": "storybook dev -p 5000",
    "storybook:build": "storybook build",
    "storybook:deploy": "npx -p @storybook/storybook-deployer storybook-to-ghpages -e storybook-static",
    "test": "jest --watch",
    "test:ci": "jest"
