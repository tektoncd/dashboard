						"id": "102511f5-3a49-407a-a985-85c792df2c24",
						"exec": [
							"pm.test(\"Response must be valid and have a body\", function () {",
							"     pm.response.to.be.ok;",
							"     pm.response.to.be.withBody;",
							"     pm.response.to.be.json;",
							"});",
							"",
							"pm.test(\"Check pipelines namespace\", function () {",
							"    var json = pm.response.json();",
							"    pm.expect(json.pipelinesNamespace).to.eql(pm.globals.get(\"pipelines_namespace\"));",
							"});",
							"",
							"pm.test(\"Check pipelines version\", function () {",
							"    var json = pm.response.json();",
							"    pm.expect(json.pipelinesVersion).to.eql(pm.globals.get(\"pipelines_version\"));",
							"});",
							""
						],
						"type": "text/javascript"
					}
				}
			],
